msgid "<p>We answer requests in:</p>\n"
msgstr "<p>Nós respondemos mensagens em:</p>\n"

#. type: Plain text
#, no-wrap
msgid ""
"<ul>\n"
"  <li>English</li>\n"
"  <li>French</li>\n"
"  <li>Spanish</li>\n"
"  <li>Italian</li>\n"
"</ul>\n"
msgstr ""
"<ul>\n"
"  <li>Inglês</li>\n"
"  <li>Francês</li>\n"
"  <li>Espanhol</li>\n"
"  <li>Italiano</li>\n"
"</ul>\n"

#. type: Plain text
#, no-wrap
msgid ""
"<p>Requests not in English might take longer to answer.\n"
"Imperfect English is welcome :)</p>\n"
msgstr ""
"<p>Mensagens que não estiverem em Inglês podem demorar mais para serem "
"respondidas.\n"
"Inglês imperfeito é bem vindo :)</p>\n"
