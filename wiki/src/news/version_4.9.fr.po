msgid "[[!meta title=\"Tails 4.9 is out\"]]\n"
msgstr "[[!meta title=\"Tails 4.9 est sorti\"]]\n"

#. type: Plain text
#, no-wrap
msgid "[[!meta date=\"Tue, 28 Jul 2020 19:00:00 +0000\"]]\n"
msgstr "[[!meta date=\"Tue, 28 Jul 2020 19:00:00 +0000\"]]\n"

