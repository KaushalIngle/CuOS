msgid "[[!meta title=\"Our achievements in 2019\"]]\n"
msgstr "[[!meta title=\"Nos réalisations en 2019\"]]\n"

#. type: Plain text
#, no-wrap
msgid "[[!meta date=\"Thu, 31 Oct 2019 12:34:56 +0000\"]]\n"
msgstr "[[!meta date=\"Thu, 31 Oct 2019 12:34:56 +0000\"]]\n"

