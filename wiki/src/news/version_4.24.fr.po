msgid "[[!meta title=\"Tails 4.24 is out\"]]\n"
msgstr "[[!meta title=\"Tails 4.24 est sorti\"]]\n"

#. type: Plain text
#, no-wrap
msgid "[[!meta date=\"Fri, 04 Nov 2021 12:34:56 +0000\"]]\n"
msgstr "[[!meta date=\"Fri, 04 Nov 2021 12:34:56 +0000\"]]\n"

